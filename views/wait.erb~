

<script type="text/javascript">
   $(function() {
      $.ajaxSetup({
        timeout:600000
      });
      setTimeout(function(){
        $('#redirect').html('If you donot get redirected to results page <a href="/results">please click here...</a>');
        },300000);
      var url=$('#url').html().text();
      $.get( url, function(data) {
        obj=JSON.parse(data);
        alert(obj.count+ " Results were successfully scraped!");
        window.location.replace("/results");
      });
   });
</script>
<div class="row">
  <br />
  <br />
  <br />
  <br />
  <br />

  <div class="span8 offset2">
    <legend>Scraping results...</legend>
    <h6>Please wait...</h6>
    <br/>
    <div class="loading"></div>
    <br />
    <p id="redirect"></p>
  </div>
</div>
<div style="display:none" id="url">
<%= @url %>
</div>
